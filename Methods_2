package act7jva;

public class PrimeChecker {
    
    public boolean NumeroPrimo(int n){
        
        if(n == 0 || n == 1 || n == 4){
            
            return false;
        }
        for(int i = 2; i < n / 2; i++){
            
            if(n % i == 0){
                
                return false;
            }
        }
        
        return true;
    }
    
}
